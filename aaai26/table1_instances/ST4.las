%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%%%%%% BACKGROUND KNOWLEDGE %%%%%%
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

num(1..4).
1{lt(X,Y); gt(X,Y); eq(X,Y)}1 :- num(X), num(Y).
lt(X,Y) :- X<Z, lt(Z,Y), num(X), num(Y), num(Z).
gt(X,Y) :- X>Z, gt(Z,Y), num(X), num(Y), num(Z).
eq(X,Y) :- X==Z, eq(Z,Y), num(X), num(Y), num(Z).

